<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <OutputType>Exe</OutputType>
    <RootNamespace>51Tracking_API</RootNamespace>
    <AssemblyName>51Tracking-API</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PackageId>51Tracking</PackageId>
    <PackageVersion>0.1.1</PackageVersion>
    <Authors>51Tracking Team</Authors>
    <Copyright>Copyright 2014-2023</Copyright>
    <Owners>51Tracking</Owners>
    <Description>51Tracking API, track your shipments</Description>
    <PackageTags>51Tracking Track Tracking Courier Couriers</PackageTags>
    <PackageIconUrl>https://s.51tracking.com/images/favicon.ico</PackageIconUrl>
    <PackageProjectUrl>https://www.51tracking.com</PackageProjectUrl>
    <Title>51Tracking</Title>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
